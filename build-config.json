{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/agendar",
      "dest": "server.js"
    },
    {
      "src": "/agendamentos",
      "dest": "server.js"
    },
    {
      "src": "/encontrar-agendamento",
      "dest": "server.js"
    },
    {
      "src": "/horarios-ocupados",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
